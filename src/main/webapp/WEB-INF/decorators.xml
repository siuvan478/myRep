<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/process/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/login*</pattern>
		<pattern>/product/scale/update/*</pattern>
		<pattern>/product/scale/create/*</pattern>
		<pattern>/order/audit/*</pattern>
		<pattern>/boxService/record/done/*</pattern>
	</excludes>
	
	<decorator name="layout" page="layout.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
